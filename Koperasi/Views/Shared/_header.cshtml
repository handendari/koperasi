@using System.Web.Optimization

<html>
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>KOPERASI KARYAWAN PT. FREIGHT EXPRESS INDONESIA GROUP</title>
		<link href='http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,400,300,600,700,800' rel='stylesheet' type='text/css'>
		<link href='http://fonts.googleapis.com/css?family=Great+Vibes' rel='stylesheet' type='text/css'>

		<link rel="shortcut icon" type="image/x-icon" href="~/content/header/image/favicon.ico">

        <link href="~/content/header/css/bootstrap.min.css" rel="stylesheet" type="text/css">
		<link href="~/content/header/css/style.css" rel="stylesheet" type="text/css">

    @Styles.Render("~/content/css")
        
        <link href="~/content/header/css/header_menu.css" rel="stylesheet" type="text/css">

		<!--[if IE 9]>
			<link rel="stylesheet" href="css/ie9.css">
		<![endif]-->
		<!--[if lt IE 9]>
            <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
			<link href="css/jackbox-ie8.css" rel="stylesheet" type="text/css" />
			<link rel="stylesheet" href="css/ie.css">
        <![endif]-->
		<!--[if gt IE 8]>
			<link href="css/jackbox-ie9.css" rel="stylesheet" type="text/css" />
		<![endif]-->
		
		<!--[if IE 7]>
			<link rel="stylesheet" href="css/fontello-ie7.css">
		<![endif]-->
		<style type="text/css">
			.no-fouc {display:none;}
	  	</style>

		<!-- jQuery -->
        @Scripts.Render("~/bundles/jquery")

		<!-- Preloader -->
		<script src="~/scripts/header/jquery.queryloader2.min.js"></script>

		<script type="text/javascript">
		    $('html').addClass('no-fouc');
		    $(document).ready(function(){
		        $('html').show();
		    });

            var base_url = '@String.Format("{0}://{1}{2}",Request.Url.Scheme, Request.Url.Authority, Url.Content("~/"))';
		</script>
	</head>

	<body class="tablet-sticky-header ">

        @RenderSection("head", required: false)
        	
			<div class="container">
				<header id="header">
					<div id="main-header">
						<div class="container">
						<div class="row">
							<div id="logo" class="col-lg-6 col-md-6 col-sm-6">
								<a href="main-v1.html" target="_blank">
                                    <img src="~/content/header/image/logo-koperasi.png" alt="Logo" height="82px">
                                    <img src="~/content/header/image/logokopkar.png" alt="Logo" height="82px">
								</a>
							</div>
							<div class="col-lg-6 col-md-6 col-sm-6 main-quote">
								<h2><b>Koperasi Karyawan<br />
								PT. FREIGHT EXPRESS INDONESIA GROUP</b></h2>
							</div>
						</div>
						</div>
					</div>
				</header>
			</div>


            <div id="wrapper">
              <nav>
                <ul class="menu">
                    <li class="home current" title="Home">
                        <a style="width:100%;height:100%" href="@Url.Action("Index", "Pelanggan")"></a>
                    </li>
                    <li ><a href="@Url.Action("ListPerCompany", "Pelanggan")">
                        <img src="@Url.Content("~/content/header/image/Parse Resumes-36.png")"> LIST SIMP & PINJ KARYAWAN
                        </a>
                    </li>
                    <li><a href="@Url.Action("Index", "User")">
                        <img src="@Url.Content("~/content/header/image/Change User-36.png")"> CHANGE PASSWORD
                        </a>
                    </li>
                    <li><a href="@Url.Action("Logout", "Login")">
                        <img src="@Url.Content("~/content/header/image/logout-36.png")"> LOG OUT
                        </a>
                    </li>
                    <li style="cursor:default; float:right">
                        <a style="cursor:default" href="#">
                            <img src="@Url.Content("~/content/header/image/user-36.png")"> @koperasi.Models.ConfigModels.GetNamaAnggota()
                        </a>
                    </li>
                </ul>
              </nav>
            </div>


        <div style="min-height:336px">

	@RenderBody()

        </div>


			<footer id="footer">
				<div id="main-footer">
                    <div style="padding-left:20px">
					    <h4 style="margin-bottom:13px !important">Koperasi Karyawan</h4>
					    <ul>
						    <li>4th Floor FEI Building</li>
						    <li>Jl. Mayjend Sungkono No. 204</li>
						    <li>Surabaya, 60225 - Indonesia - P. +6231-561-6465, F. +6231-561-9177</li>
					    </ul>
                    </div>
				</div>
			</footer>

        <script type="text/javascript" src="~/scripts/header/modernizr.js"></script>

		<!--[if lt IE 9]>
			<script type="text/javascript" src="js/jquery.placeholder.js"></script>
			<script type="text/javascript" src="js/script_ie.js"></script>
		<![endif]-->
	</body>
</html>